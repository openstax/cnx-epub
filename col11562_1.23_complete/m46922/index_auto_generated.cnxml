 <m:mover accent="true">
  <m:mi>x</m:mi>
  <m:mo>&#175;</m:mo>
 </m:mover>
 </m:math>)</entry>
    <entry>(<emphasis effect="italics">x</emphasis> &#8211; <m:math>
 <m:mover accent="true">
