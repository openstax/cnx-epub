<entry>&gt; 10</entry>
<entry>empty</entry>
<entry>empty</entry>
<entry>empty</entry>
</row>
</tbody>
</tgroup>
</table>
</problem>
<solution id="fs-idm55204288">
<table id="fs-idp43286224" summary="..">
<title/>
<tgroup cols="4">
