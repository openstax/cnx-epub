<m:mrow>
<m:mi>P</m:mi><m:mo stretchy="false">(</m:mo><m:mi>B</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
<m:mrow>
<m:mn>25</m:mn>
</m:mrow>
<m:mrow>
<m:mn>90</m:mn>
</m:mrow>
</m:mfrac>
<m:mo>=</m:mo><m:mn>0.28</m:mn>
</m:mrow>
</m:math>
</para>
</solution>
</exercise>
