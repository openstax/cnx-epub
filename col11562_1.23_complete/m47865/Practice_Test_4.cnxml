<m:mi>r</m:mi>
<m:mn>2</m:mn>
</m:msup>

</m:mrow>
</m:msqrt>

</m:mrow>
</m:mfrac>
<m:mo>=</m:mo><m:mfrac>
<m:mrow>
<m:mn>0.45</m:mn><m:msqrt>
<m:mrow>
<m:mn>25</m:mn><m:mo>&#x2212;</m:mo><m:mn>2</m:mn>
</m:mrow>
</m:msqrt>

</m:mrow>
<m:mrow>
<m:msqrt>
