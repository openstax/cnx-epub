<m:mrow>
<m:mi>t</m:mi><m:mo>=</m:mo><m:mfrac>
<m:mrow>
<m:mi>r</m:mi><m:msqrt>
<m:mrow>
<m:mi>n</m:mi><m:mo>&#x2212;</m:mo><m:mn>2</m:mn>
</m:mrow>
</m:msqrt>

