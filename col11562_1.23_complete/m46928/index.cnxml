<solution id="fs-idp56820112">
<list id="fs-idp26414448" list-type="bulleted">
<item>Find the midpoints for all intervals</item>
</list>
<table id="fs-idp15194576" summary="">
<tgroup cols="2">
<colspec colnum="1" colname="c1"/>
<colspec colnum="2" colname="c2"/>
<thead>
<row><entry>Grade Interval</entry>	<entry>Midpoint</entry></row>
