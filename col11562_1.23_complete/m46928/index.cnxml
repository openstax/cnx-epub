    <m:mrow>
     <m:mstyle displaystyle="true">
      <m:mo>&#8721;</m:mo> <m:mrow>
       <m:mi>f</m:mi><m:mi>m</m:mi>
      </m:mrow>
     </m:mstyle>
    </m:mrow>
    <m:mrow>
     <m:mstyle displaystyle="true">
      <m:mo>&#8721;</m:mo> <m:mi>f</m:mi>
     </m:mstyle>
    </m:mrow>
   </m:mfrac>

  </m:mrow>

</m:math> Where <emphasis effect="italics">f</emphasis> = interval frequencies and <emphasis effect="italics">m</emphasis> = interval midpoints.</para></section>

