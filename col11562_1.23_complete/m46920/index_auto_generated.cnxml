<item>60</item>
<item>61</item>
<item>62</item>
<item>62</item>
<item>63</item>
<item>63</item>
<item>64</item>
<item>64</item>
<item>64</item>
<item>65</item>
<item>65</item>
<item>65</item>
<item>65</item>
<item>65</item>
<item>65</item>
<item>65</item>
<item>65</item>
<item>65</item>
<item>66</item>
<item>66</item>
<item>67</item>
<item>67</item>
<item>68</item>
<item>68</item>
<item>69</item>
<item>70</item>
<item>70</item>
<item>70</item>
<item>70</item>
<item>70</item>
<item>71</item>
<item>71</item>
