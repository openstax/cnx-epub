   <m:mrow>
    <m:mn>36</m:mn>
   </m:mrow>
  </m:mfrac>
 </m:mrow>
</m:math> = 0</entry></row>
<row>
<entry>2</entry>	<entry><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mn>9</m:mn>
   <m:mrow>
    <m:mn>36</m:mn>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</entry>	<entry><m:math>
 <m:mrow>
  <m:mfrac>
   <m:mn>18</m:mn>
   <m:mrow>
    <m:mn>36</m:mn>
   </m:mrow>
